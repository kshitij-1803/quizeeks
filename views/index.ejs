        <ul class="nav-items">
          <li><a href="#">Study Resources</a></li>
          <li><a href="/highscores">High Scores</a></li>
          <% if (user) {%> 
            <div class="active-user">
                <li class="username"><%=user.name%> 
                  <img src="/img/down-arrow.svg" alt="">  
                </li>
                <div class="user-options">
                  <li><a href ="/myquizes">Your Quizes</a></li>
                  <li><a href ="/changepass">Change password</a></li>
                  <li><a href ="/logout">Log Out</a></li>
                </div>
            </div>
          <% } else { %> 
